<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        android:id="@+id/overlay_bilibili_priconne_card_1"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_marginEnd="11.5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@id/overlay_bilibili_priconne_card_2"
        android:src="@drawable/shape_bilibili_priconne_card"
        android:background="@color/transparent"/>
    <ImageView
        android:id="@+id/overlay_bilibili_priconne_card_2"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_marginEnd="11.5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@id/overlay_bilibili_priconne_card_3"
        android:src="@drawable/shape_bilibili_priconne_card"
        android:background="@color/transparent"/>
    <ImageView
        android:id="@+id/overlay_bilibili_priconne_card_3"
        android:layout_width="76dp"
        android:layout_height="76dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/shape_bilibili_priconne_card"
        android:background="@color/transparent"/>
    <ImageView
        android:id="@+id/overlay_bilibili_priconne_card_4"
        android:layout_width="76dp"
        android:layout_height="76dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/overlay_bilibili_priconne_card_3"
        android:layout_marginStart="11.5dp"
        android:src="@drawable/shape_bilibili_priconne_card"
        android:background="@color/transparent"/>
    <ImageView
        android:id="@+id/overlay_bilibili_priconne_card_5"
        android:layout_width="76dp"
        android:layout_height="76dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/overlay_bilibili_priconne_card_4"
        android:layout_marginStart="11.5dp"
        android:src="@drawable/shape_bilibili_priconne_card"
        android:background="@color/transparent"/>
</androidx.constraintlayout.widget.ConstraintLayout>