    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:id="@+id/overlay_bilibili_priconne_switcher_5"
        android:layout_width="76dp"
        android:layout_height="30dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/overlay_bilibili_priconne_switcher_4"
        android:layout_marginStart="11.5dp">
        <TextView
            android:id="@+id/overlay_bilibili_priconne_switcher_5_title"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textStyle="bold"
            android:textSize="16sp"
            android:gravity="center" />
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>